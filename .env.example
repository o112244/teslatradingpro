# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Configuration (Optional - for production backend)
VITE_API_URL=https://your-backend-api.com
VITE_WS_URL=wss://your-websocket-server.com/ws

# External API Keys (Optional - for live market data)
VITE_COINGECKO_API_KEY=your_coingecko_api_key
VITE_FMP_API_KEY=your_financial_modeling_prep_api_key

# Authentication (Optional - for JWT)
VITE_JWT_SECRET=your_jwt_secret_key

# Bitcoin Wallet Configuration (Optional)
VITE_BITCOIN_NETWORK=mainnet
VITE_ADMIN_WALLET_ADDRESS=your_admin_bitcoin_address

# Payment Processing (Optional - for Stripe)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# Environment
NODE_ENV=production